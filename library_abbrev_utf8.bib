@Article{perrasNaturalAbundance17O2015,
  author   = {Perras, Fr{\'{e}}d{\'{e}}ric A and Kobayashi, Takeshi and Pruski, Marek},
  title    = {{Natural {\{}{\{}Abundance 17O DNP Two{\}}{\}}-{\{}{\{}Dimensional{\}}{\}} and {\{}{\{}Surface{\}}{\}}-{\{}{\{}Enhanced NMR Spectroscopy{\}}{\}}}},
  journal  = {J. Am. Chem. Soc.},
  year     = {2015},
  volume   = {137},
  number   = {26},
  pages    = {8336--8339},
  month    = {jul},
  issn     = {0002-7863},
  abstract = {Due to its extremely low natural abundance and quadrupolar nature, the 17O nuclide is very rarely used for spectroscopic investigation of solids by NMR without isotope enrichment. Additionally, the applicability of dynamic nuclear polarization (DNP), which leads to sensitivity enhancements of 2 orders of magnitude, to 17O is wrought with challenges due to the lack of spin diffusion and low polarization transfer efficiency from 1H. Here, we demonstrate new DNP-based measurements that extend 17O solid-state NMR beyond its current capabilities. The use of the PRESTO technique instead of conventional 1H$\backslash$textendash{\{}{\}}17O cross-polarization greatly improves the sensitivity and enables the facile measurement of undistorted line shapes and two-dimensional 1H$\backslash$textendash{\{}{\}}17O HETCOR NMR spectra as well as accurate internuclear distance measurements at natural abundance. This was applied for distinguishing hydrogen-bonded and lone 17O sites on the surface of silica gel; the one-dimensional spectrum of which could not be used to extract such detail. Lastly, this greatly enhanced sensitivity has enabled, for the first time, the detection of surface hydroxyl sites on mesoporous silica at natural abundance, thereby extending the concept of DNP surface-enhanced NMR spectroscopy to the 17O nuclide.},
  doi      = {10.1021/jacs.5b03905},
}
