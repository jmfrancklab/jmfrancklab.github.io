journal = {Journal of Magnetic Resonance, Series A},
keywords = {DNP Inst.},
mendeley-tags = {DNP Inst.},
number = {1},
pages = {28--40},
title = {{A Spectrometer for Dynamic Nuclear Polarization and Electron Paramagnetic Resonance at High Frequencies}},
