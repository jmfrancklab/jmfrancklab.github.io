  keywords   = {1H,2D DOSY NMR,LC-NMR,NMR,acid,base,electrophoretic NMR,electrophoretic mobility,ion,self-diffusion},
  shorttitle = {Assessing {\{}{\{}2D{\}}{\}} Electrophoretic Mobility Spectros},
