issn = {0947-6539},
journal = {Chemistry (Weinheim an der Bergstrasse, Germany)},
keywords = {cage},
month = {jan},
number = {6},
pages = {1715--23},
shorttitle = {Chemistry},
title = {{Long-range-distance NMR effects in a protein labeled with a lanthanide-DOTA chelate.}},
volume = {13},
year = {2007}
}
