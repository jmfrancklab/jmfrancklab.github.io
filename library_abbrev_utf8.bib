@Article{Bagchi1984,
  author   = {Bagchi, Biman and Oxtoby, David W and Fleming, Graham R},
  title    = {{Theory of the Time Development of the Stokes Shift in Polar Media}},
  journal  = {Chem. Phys.},
  year     = {1984},
  volume   = {86},
  number   = {3},
  pages    = {257--267},
  month    = {jan},
  issn     = {0301-0104},
  abstract = {We present a theory for the time evolution of the Stokes shift of a polar molecule in a polar solvent. The time-dependent solute$\backslash$textemdash{\{}{\}}solvent interaction is calculated in a continuum model by replacing the surrounding solvent by a frequency-dependent dielectric continuum. An expression for the time dependence of the fluorescence maximum is derived. This expression can be considered a direct generalization of the well-known Ooshika$\backslash$textemdash{\{}{\}}Lippert$\backslash$textemdash{\{}{\}}Mataga equation to the time domain. We also present an approximate expression for the wavelength dependence of the dynamics of the Stokes shift, and find it to be consistent with recent experimental results. We have investigated the effect of polarizability of the solute molecule and found that for many molecules this effect is not negligible.},
  doi      = {10.1016/0301-0104(84)80014-2},
}

