month = {jul},
number = {7},
pages = {793--9},
pmid = {21308416},
title = {{Low intensity microwave radiation as modulator of the L-lactate dehydrogenase activity.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21308416},
volume = {49},
year = {2011}
}
