}

@Article{VanHal1999,
  author  = {van Hal, Paul a. and Christiaans, Marwijn P. T. and Wienk, Martijn M. and Kroon, Jan M. and Janssen, Ren{\'{e}} a. J.},
  title   = {{Photoinduced Electron Transfer from Conjugated Polymers to TiO 2}},
  journal = {J. Phys. Chem. B},
  year    = {1999},
  volume  = {103},
