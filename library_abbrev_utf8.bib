month = {jan},
number = {7},
pages = {1320--6},
pmid = {15590285},
title = {{Theoretical and experimental study towards a nanogap dielectric biosensor.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15590285},
volume = {20},
year = {2005}
}
