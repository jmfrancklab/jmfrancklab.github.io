@Article{deAlmeidaMartins2016,
  author   = {{de Almeida Martins}, Jo{\~{a}}o P and Topgaard, Daniel},
  title    = {{Two-{\{}{\{}Dimensional Correlation{\}}{\}} of {\{}{\{}Isotropic{\}}{\}} and {\{}{\{}Directional Diffusion Using NMR{\}}{\}}}},
