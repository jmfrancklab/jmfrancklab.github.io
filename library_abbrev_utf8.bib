  file     = {:C$\backslash$:/Users/johnf/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Vassiliev, Zaraiskaya, Bruce - 2012 - Exploring the energetics of water permeation in photosystem II by multiple steered molecular dynam.pdf:pdf},
  keywords = {Binding Sites,Energy Metabolism,Molecular Dynamics Simulation,Permeability,Photosynthesis,Photosystem II Protein Complex,Photosystem II Protein Complex: metabolism,Water,Water: chemistry,Water: metabolism},
