@Article{Zhou2013,
  author        = {Zhou, Shaobing and Fan, Jing and Datta, Sujit S. and Guo, Ming and Guo, Xing and Weitz, David A.},
  title         = {{Thermally Switched Release from Nanoparticle Colloidosomes}},
  journal       = {Adv. Funct. Mater.},
