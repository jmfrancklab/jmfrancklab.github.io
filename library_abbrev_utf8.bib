@Article{Flores2003,
  author   = {Flores, M and Isaacson, R A and Calvo, R and Feher, G and Lubitz, W},
  title    = {{Probing hydrogen bonding to quinone anion radicals by 1H and 2H ENDOR spectroscopy at 35 GHz}},
